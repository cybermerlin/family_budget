  "resolutions": {
    "jest": "29.3.1",
    "jest-config": "^29.3.1",
    "jest-util": "29.3.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,gql,graphql,scss,css}": "eslint --color"
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  },
  "config": {
    "pre-git": {
      "enabled": true,
      "allow-untracked-files": true,
      "msg-pattern": "^#(\\W-)?\\d+ (fix|add|del|mod): ([^>]+?)(> [^>]+?)?$",
      "commit-msg": "",
      "post-pull": [
        "npm i"
      ],
			"pre-commit": [
				"npm run lint-staged"
			],
			"post-commit": [
				"git status",
				"git update-index --again"
			],
			"pre-push": [
				"npm test"
			]
    }
  }
